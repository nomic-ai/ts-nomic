{
  "name": "@nomic-ai/atlas",
  "version": "0.2.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "scripts": {
    "test": "env-cmd --file .env uvu -r ./es-build-test-config.js tests",
    "build-types": "openapi-typescript etc/openapi.json > etc/openapi.d.ts",
    "build": "tsc && npm run build:esbuild",
    "build:esbuild": "esbuild --outdir=dist --format=esm --platform=node src/main.ts",
    "build:watch": "tsc -w",
    "precommit": "pretty-quick --staged",
    "prepare": "husky install .husky"
  },
  "devDependencies": {
    "esbuild": "^0.18.9",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "typescript": "^5.1.3",
    "uvu": "^0.5.6"
  },
  "dependencies": {
    "apache-arrow": "^12.0.1",
    "dotenv": "^16.3.1",
    "js-yaml": "^4.1.0"
  }
}
