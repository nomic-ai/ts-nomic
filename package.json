    "env-cmd": "^10.1.0",
    "esbuild": "^0.17.17",
    "esbuild-jest": "^0.5.0",
    "esbuild-register": "^3.4.2",
    "husky": "^8.0.3",
    "jasmine": "^3.99.0",
    "jasmine-ts": "^0.4.0",
    "jest": "^29.5.0",
    "openapi-typescript": "^6.2.0",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.5",
    "uvu": "^0.5.6"
