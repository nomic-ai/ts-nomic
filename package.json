{
  "name": "@nomic-ai/atlas",
  "version": "0.8.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/main.js",
  "types": "./dist/main.d.ts",
  "exports": {
    ".": {
      "import": "./dist/main.js"
    }
  },
  "scripts": {
    "pretest": "npm run build",
